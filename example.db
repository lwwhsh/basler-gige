## Read current mode remote/local.
record(waveform,"TL1-DI-CAM1:getImage")
{
	field(DESC,	"Captures a single frame")
	field(DTYP, "basler")
	field(INP,  "@C1:capture")
	field(FTVL, "UCHAR")
	field(NELM, "1251936")
}

record(longin, "TL1-DI-CAM1:getExposure")
{
	field(SCAN, "5 second")
	field(DTYP, "basler")
	field(INP,  "@C1:getExposure")
}

record(longout, "TL1-DI-CAM1:setExposure")
{
	field(DTYP, "basler")
	field(OUT,  "@C1:setExposure")
}

